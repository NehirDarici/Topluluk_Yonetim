<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<!-- DÜZELTME 1: fx:id="anaIcerik" buradan kaldırıldı -->

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="500.0" prefWidth="810.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.proje.controller.EtkinlikBaskanController">
   <left>
      <VBox prefHeight="200.0" prefWidth="170.0" style="-fx-background-color: #2D3447;" BorderPane.alignment="CENTER">
